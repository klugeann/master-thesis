# Environment variables for survey.py

# Directory containing the survey files
export SURVEY_DIRECTORY=survey1

# Use causal inference (y/N)
export USE_CAUSAL_INFERENCE=n

# Path to causal inference data (if applicable)
export CAUSAL_INFERENCE_DATA=causal_inference_consumer_data.csv

# Dry run mode (y/N)
export DRY_RUN=n

# OpenAI API key
export OPENAI_API_KEY=llm-studio

# OpenAI base URL (if applicable)
export OPENAI_BASE_URL=http://localhost:2802/v1

# OpenAI model
export OPENAI_MODEL=dllama-3.2-3b-instruct

# Number of trials
export NUM_TRIALS=100

# Within-subject survey (y/N)
export WITHIN_SUBJECT=n

# Output file name
export OUTPUT_FILE=pilot_final_survey1_normal.csv